{
  "name": "re-food",
  "description": "Food dontation app project for CITS3200 🍉",
  "version": "0.1.0",
  "private": true,
  "pre-commit": [
    "lint",
    "format",
    "test-changed"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "TZ=UTC jest --ci --maxWorkers=4",
    "test-changed": "TZ=UTC jest -o",
    "format": "prettier --log-level warn --write \"src/{components,pages,hooks,types}/**/*.{tsx,ts}\"",
    "gen-types-file": "eval $(grep '^PROJECT_REF' .env) && supabase gen types typescript --project-id \"$PROJECT_REF\" > src/types/supabase.d.ts",
    "gen-types": "npm run gen-types-file  && node src/types/cleanTypes.js  &&  node src/types/supabaseInterfaces.js && prettier --write src/types/{supabase,supabaseTables}.d.ts",
    "vapid": "web-push generate-vapid-keys"
  },
  "engines": {
    "node": "^18"
  },
  "jest-junit": {
    "outputDirectory": "./node_modules/.cache/test-reports"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/supabase-js": "^2.32.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "browser-image-compression": "^2.0.2",
    "classcat": "^5.0.4",
    "d3-scale-chromatic": "^3.0.0",
    "date-fns": "^2.30.0",
    "ez-scss": "^1.0.7",
    "file-saver": "^2.0.5",
    "formik": "^2.4.3",
    "graphql": "^16.7.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.15.0",
    "matter-js": "^0.19.0",
    "next": "^13.4.12",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-map-gl-geocoder": "^2.2.0",
    "react-modal": "^3.16.1",
    "react-spinners": "^0.13.8",
    "react-switch": "^7.0.0",
    "react-use": "^17.4.0",
    "recharts": "^2.8.0",
    "sass": "^1.64.2",
    "uuid": "^9.0.0",
    "web-push": "^3.6.6",
    "webpack": "^5.88.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@svgr/webpack": "^8.0.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest-image-snapshot": "^6.2.0",
    "@types/lodash": "^4.14.197",
    "@types/mapbox-gl": "^2.7.14",
    "@types/matter-js": "^0.19.1",
    "@types/react": "^18.0.29",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^9.0.2",
    "@types/web-push": "^3.6.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^6.2.0",
    "jest-junit": "^16.0.0",
    "jest-transform-stub": "^2.0.0",
    "next-router-mock": "^0.9.8",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.1",
    "supabase": ">=1.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
